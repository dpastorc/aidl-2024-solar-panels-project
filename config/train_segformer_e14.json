{
    "model": "Segformer",
    "pre_trained": "segformer_solar_panel_detector_E13.pth",
    "batch_size": 64,
    "epochs": 40,
    "lr": 0.0001,
    "augmentation": {
        "flip": true,
        "rotation": true,
        "brightness": true,
        "contrast": true,
        "saturation": true,
        "hue": true,
        "blur": true,
        "sharpen": true,
        "gaussian_noise": true,
        "random_padding": true,
        "random_polygons": true,
        "contain_solar_panel": true
    },
    "validation_interval": 8,
    "num_samples": 10
}
